{
  "name": "linda",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "APP_VARIANT=development npx expo start",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{jsi,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "test": "jest",
    "doctor": "npx expo-doctor",
    "clean": "if [ -d \"node_modules\" ]; then rm -rf node_modules && rm package-lock.json && npm cache clean --force; else echo 'No node_modules to clean'; fi",
    "healthcheck": "npm run doctor && npm run lint && npm run test",
    "prepare": "husky"
  },
  "dependencies": {
    "@sentry/react-native": "~6.3.0",
    "expo": "^52.0.0",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.19",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "nativewind": "^4.1.23",
    "react": "^18.3.1",
    "react-native": "0.76.9",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true
}
